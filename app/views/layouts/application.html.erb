    <%= csrf_meta_tags %>
  </head>
  <body>
    <div id="header">
      <%= link_to 'Stranger Roads', root_path, class: 'slide under' %>
    </div>
    <div id="slider" class="container">
      <%= yield(:additional_javascripts) %>
    </div>
  </body>
</html>
